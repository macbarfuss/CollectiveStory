<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui'>
	<g:HTMLPanel>
		<table>
			<tr>
				<td colspan="2"><g:Label ui:field="message"></g:Label></td>
			</tr>
			<tr>
				<td>
					<g:Label>Name</g:Label>
				</td>
				<td>
					<g:TextBox ui:field="usernameBox" width="15em" />
				</td>

			</tr>
			<tr>
				<td>
					<g:Label>Password</g:Label>
				</td>
				<td>
					<g:PasswordTextBox ui:field="passphraseBox" width="15em" />
				</td>
			</tr>
			<tr>
				<td colspan='2'>
					<g:Button ui:field="btnLogin" text="login" />
				</td>
			</tr>
		</table>
	</g:HTMLPanel>
</ui:UiBinder>
